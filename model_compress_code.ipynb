   "source": [
    "import tarfile \n",
    "\n",
    "import os.path \n",
    "\n",
    "def make_tarfile(output_filename, source_dir): \n",
    "\n",
    "    with tarfile.open(output_filename, \"w:gz\") as tar: \n",
    "\n",
    "        tar.add(source_dir, arcname=os.path.basename(source_dir)) \n",
    "\n",
    "make_tarfile(\"model.tar.gz\", \"mobile_price_rg.pkl\")"
   ]
